{
    "name": "frontend",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "predev": "npm run gen",
        "prebuild": "npm run gen",
        "dev": "vite",
        "type-check": "vue-tsc --noEmit",
        "build": "vue-tsc && vite build",
        "preview": "vite preview",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "gen:swagger": "powershell -NoProfile -ExecutionPolicy Bypass -File ../swagger/gen-swagger.ps1",
        "gen:api": "npx --yes swagger-typescript-api generate -p ../swagger/swagger.json -o src/shared/api/generated -n index.ts --unwrap-response-data",
        "gen": "npm run gen:swagger && npm run gen:api"
    },
    "dependencies": {
        "pinia": "^3.0.4",
        "vue": "^3.5.13",
        "vue-router": "^4.5.0"
    },
    "devDependencies": {
        "@eslint/js": "^9.39.2",
        "@types/node": "^25.0.9",
        "@vitejs/plugin-vue": "^5.2.1",
        "eslint": "^9.39.2",
        "eslint-plugin-vue": "^10.7.0",
        "swagger-typescript-api": "^13.2.10",
        "typescript": "^5.6.3",
        "typescript-eslint": "^8.53.1",
        "vite": "^6.0.5",
        "vue-tsc": "^2.1.10"
    }
}